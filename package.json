{
  "name": "lan-play-server-manager",
  "version": "0.0.1",
  "description": "A project to manager servers",
  "main": "main.js",
  "scripts": {
  "postinstall": "install-app-deps",
    "start": "electron .",
	"pack": "build --dir",
	"dist": "build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/takashi1kun/lan-play-server-watcher.git"
  },
  "author": "Aitor Rosell Torralba",
  "license": "GPL-3.0",
  "build": {
    "appId": "com.lanplayservermanager.app",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico"
    }
  },
  "devDependencies": {
    "bootstrap": "^4.1.3",
    "electron": "^3.0.10",
    "electron-builder": "^20.38.2",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.6"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "child_process": "^1.0.2",
    "electron-fetch": "^1.3.0",
    "electron-json-config": "^1.5.3",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.6"
  }
}
